        "@saeris/vue-spinners": "^1.0.8",
        "@xkeshi/vue-countdown": "^1.0.1",
        "laravel-vue-pagination": "^2.3.0",
        "sweetalert": "^2.1.2",
        "vue": "^2.6.10",
        "vue-moment": "^4.0.0",
        "vue-phone-number-input": "^0.1.8",
